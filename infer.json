{
  "pipeline": {
    "name": "inference"
  },
  "transform": {
    "image": "dwhitena/pachr:svm",
    "cmd": [ 
	"Rscript",
	"infer.R"
    ]
  },
  "parallelism_spec": {
    "strategy": "CONSTANT",
    "constant": "1"
  },
  "inputs": [
    {
      "repo": {
        "name": "attributes"
      },
      "glob": "/*"
    },
    {
      "repo": {
	"name": "model"
      },
      "glob": "/"
    }
  ]
}
